# program to add all number using recursion
def sum1(n):
    if n == 0:
        return 0
    else:
        return sum1(n-1)+n
print(sum1(10)) 


# program to get binary number recursion
def binar(n):
    if n == 0:
        return '0'
    else:
        return binar(n // 2) + str(n % 2)

a = binar(5)
print(a)
    
